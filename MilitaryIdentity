public class Military extends Person{
private String status;

public Military(String f, String m, String l, String g, String s)
{
super(f, m, l, g);
status = s;
}
public String toString()
{
return String.format(" %s, Status: %s", super.toString(), status);
}
}
public class NavySailor extends Military{
    private String assignment;
    private String rank
    public NavySailor(String f, String m, String l, String g, String s, String a, String r)
    {
        super(f, m, l, g, s);
        assignment = a;
        rank = r; 
    }
    public String toString()
    {
        return String.format(" %s, Assignment: %s, Rank: %s", super.toString(), assignment, rank);
    }
}
